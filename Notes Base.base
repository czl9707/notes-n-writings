filters:
  and:
    - file.inFolder("as/developer/notes")
formulas:
  Note: link(file.basename, title)
views:
  - type: table
    name: Table
    order:
      - formula.Note
      - tags
      - created_date
      - last_modified_date
    sort:
      - property: created_date
        direction: DESC
    columnSize:
      formula.Note: 499
      note.tags: 283
      note.last_modified_date: 158
