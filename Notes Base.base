filters:
  and:
    - file.inFolder("note/by/developer")
    - '!file.inFolder("note/by/developer/drafts")'
formulas:
  Note: link(file.basename, title)
views:
  - type: table
    name: Table
    order:
      - formula.Note
      - tags
      - created-date
      - last-modified-date
    sort:
      - property: created-date
        direction: DESC
    columnSize:
      formula.Note: 499
      note.tags: 283
      note.last-modified-date: 158
